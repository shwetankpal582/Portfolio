{
  "functions": {
    "backend/api/**/*.ts": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles": [
        "backend/dist/lib/**"
      ],
      "launcherType": "Nodejs"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/$1"
    }
  ]
}
